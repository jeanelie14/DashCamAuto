{
  "name": "DashCamAuto",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "react-native clean",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "build:android": "cd android && ./gradlew assembleRelease && cd ..",
    "build:ios": "cd ios && xcodebuild -workspace DashCamAuto.xcworkspace -scheme DashCamAuto -configuration Release && cd ..",
    "check:deps": "npm outdated",
    "postinstall": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native/new-app-screen": "0.81.4",
    "react": "18.2.0",
    "react-native": "0.81.4",
    "react-native-device-info": "10.11.0",
    "react-native-fs": "2.20.0",
    "react-native-geolocation-service": "5.3.1",
    "react-native-permissions": "3.10.1",
    "react-native-sensors": "7.0.0",
    "react-native-camera": "4.2.1",
    "react-redux": "8.1.3",
    "@reduxjs/toolkit": "1.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.4",
    "@react-native/eslint-config": "0.81.4",
    "@react-native/metro-config": "0.81.4",
    "@react-native/typescript-config": "0.81.4",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.13",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.6.3",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20"
  }
}
